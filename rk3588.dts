# 15 "rk3588-orangepi-5-plus.dts" 2

/ {
 model = "Xunlong Orange Pi 5 Plus";
 compatible = "xunlong,orangepi-5-plus", "rockchip,rk3588";

 aliases {
  mmc0 = &sdhci;
  mmc1 = &sdmmc;
 };

 chosen {
  stdout-path = "serial2:1500000n8";
 };

 adc-keys-0 {
  compatible = "adc-keys";
  io-channels = <&saradc 0>;
  io-channel-names = "buttons";
  keyup-threshold-microvolt = <1800000>;
  poll-interval = <100>;

  button-maskrom {
   label = "Mask Rom";
   linux,code = <141>;
   press-threshold-microvolt = <2000>;
  };
 };

 adc-keys-1 {
  compatible = "adc-keys";
  io-channels = <&saradc 1>;
  io-channel-names = "buttons";
  keyup-threshold-microvolt = <1800000>;
  poll-interval = <100>;

  button-recovery {
   label = "Recovery";
   linux,code = <0x168>;
   press-threshold-microvolt = <2000>;
  };
 };

 speaker_amp: speaker-audio-amplifier {
  compatible = "simple-audio-amplifier";
  enable-gpios = <&gpio3 16 0>;
  sound-name-prefix = "Speaker Amp";
 };

 headphone_amp: headphones-audio-amplifier {
  compatible = "simple-audio-amplifier";
  enable-gpios = <&gpio3 7 0>;
  sound-name-prefix = "Headphones Amp";
 };

 ir-receiver {
  compatible = "gpio-ir-receiver";
  gpios = <&gpio4 11 1>;
  pinctrl-names = "default";
  pinctrl-0 = <&ir_receiver_pin>;
 };

 gpio-leds {
  compatible = "gpio-leds";
  pinctrl-names = "default";
  pinctrl-0 = <&blue_led_pin>;

  led {
   color = <3>;
   function = "indicator";
   function-enumerator = <1>;
   gpios = <&gpio3 6 0>;
  };
 };
# 100 "rk3588-orangepi-5-plus.dts"
 fan: pwm-fan {
  compatible = "pwm-fan";
  cooling-levels = <0 70 75 80 100>;
  fan-supply = <&vcc5v0_sys>;
  pwms = <&pwm3 0 50000 0>;
  #cooling-cells = <2>;
 };

 pwm-leds {
  compatible = "pwm-leds";

  led {
   color = <2>;
   function = "indicator";
   function-enumerator = <2>;
   max-brightness = <255>;
   pwms = <&pwm2 0 25000 0>;
  };
 };

 rfkill {
  compatible = "rfkill-gpio";
  label = "rfkill-pcie-wlan";
  radio-type = "wlan";
  shutdown-gpios = <&gpio0 20 0>;
 };

 sound {
  compatible = "simple-audio-card";
  pinctrl-names = "default";
  pinctrl-0 = <&hp_detect>;
